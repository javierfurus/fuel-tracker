{
  "swagger": "2.0",
  "info": {
    "title": "Fuel Tracking App"
  },
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/": {
      "get": {
        "summary": "Hello World",
        "operationId": "helloWorld",
        "responses": {
          "200": {
            "description": "Return with Hello World message"
          }
        }
      }
    },
    "/track": {
      "get": {
        "summary": "Show tracked values",
        "operationId": "track",
        "responses": {
          "200": {
            "description": "Return with tracked values"
          }
        }
      },
      "post": {
        "summary": "Create new values",
        "operationId": "addToTracking",
        "responses": {
          "200": {
            "description": "Trackeable values added"
          }
        }
      },
      "delete": {
        "summary": "Delete existing tracked record",
        "operationId": "deleteFromTracking",
        "responses": {
          "200": {
            "description": "Tracked value deleted"
          }
        }
      }
    }
  }
}
